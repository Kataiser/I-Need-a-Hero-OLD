        window_set_cursor(cr_handpoint)
        //hover = random_range(2,20)
        hover = round((mouse_y - 145) / 25)
        if hover = -1 {hover = 0}
        if mouse_check_button_pressed(mb_left) {
            selecting = false
            return_to.hero = hover
            return_to.focus = 0
            return_to = noone
        }
    }
    else {
        window_set_cursor(cr_default)
    }
}
