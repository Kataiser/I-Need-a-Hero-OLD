finallist = ds_grid_create(22,3)
for (i = 0; i &lt; 21; i += 1) {
    ds_grid_add(finallist,i,0,0)
    ds_grid_add(finallist,i,1,0)
    ds_grid_add(finallist,i,2,0)
}

file = file_text_open_read(working_directory + "\counters.txt")
hero_counters = ds_grid_create(23,23)
for (i = 0; i &lt; 23; i += 1) { //vertical
    line = file_text_read_string(file)
    file_text_readln(file)
    for (j = 0; j &lt; 100; j += 1) { //horizontal
        write = 0
        if (string_char_at(line,j) = "") {break}
        else if (string_char_at(line,j) = "-") {
            j += 1
            write = -string_char_at(line,j)
        }
        else {write = string_char_at(line,j)}
        ds_grid_set(hero_counters,j,i,write) //write to the grid
    }
}
file_text_close(file)
