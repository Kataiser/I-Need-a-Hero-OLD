            <string>col_ltgrey = make_colour_rgb(220,220,220)
col_overwatch = make_colour_rgb(255,241,165)
selecting = false
return_to = noone
hover = 0
unknown = true
randomize()

heroes = ds_list_create()
ds_list_add(heroes,"Genji","McCree","Pharah","Reaper","Soldier: 76","Sombra","Tracer","Bastion","Hanzo","Junkrat","Mei","Torbjorn","Widowmaker","D.Va","Reinhardt","Roadhog","Winston","Zarya","Ana","Lucio","Mercy","Symmetra","Zenyatta")
friendlyteam = ds_list_create()
ds_list_add(friendlyteam,-1,-1,-1,-1,-1,-1)
enemyteam = ds_list_create()
ds_list_add(enemyteam,-1,-1,-1,-1,-1,-1)
finallist = ds_grid_create(4,22)
for (i = 0; i &lt; 22; i += 1) {
    ds_grid_set(finallist,0,i,i) //hero (for sorting)
    ds_grid_set(finallist,1,i,0) //main score
    ds_grid_set(finallist,2,i,0) //counters
    ds_grid_set(finallist,3,i,0) //synergies
}

file = file_text_open_read(working_directory + "\counters.txt")
counters = ds_grid_create(22,22)
for (i = 0; i &lt; 23; i += 1) { //vertical
    line = file_text_read_string(file)
    file_text_readln(file)
    k = -1 //I don't remember why this starts at -1
    for (j = 0; j &lt; string_length(line); j += 1) { //horizontal
        write = 9
        if (string_char_at(line,j) = "") {break} //end of line
        if (string_char_at(line,j) = "-") {
            j += 1
            write = -real(string_char_at(line,j))
        }
        else {write = real(string_char_at(line,j))}
        ds_grid_set(counters,i,k,write) //write to the grid
        k += 1
    }
}
file_text_close(file)
</string>
